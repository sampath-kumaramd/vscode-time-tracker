{
     "name": "vscode-time-tracker",
     "displayName": "VS Code Time Tracker",
     "description": "A simple time tracking extension for VS Code",
     "version": "0.1.0",
     "publisher": "YourPublisherName",
      "license": "MIT",
     "engines": {
       "vscode": "^1.60.0"
     },
     "categories": [
       "Other"
     ],
     "activationEvents": [
       "onStartupFinished"
     ],
     "main": "./out/extension.js",
     "contributes": {
       "commands": [
         {
           "command": "time-tracker.start",
           "title": "Start Time Tracking"
         },
         {
           "command": "time-tracker.stop",
           "title": "Stop Time Tracking"
         },
         {
           "command": "time-tracker.manualEntry",
           "title": "Add Manual Time Entry"
         },
         {
           "command": "time-tracker.dailyReport",
           "title": "Show Daily Time Tracking Report"
         },
         {
           "command": "time-tracker.allEntries",
           "title": "Show All Time Tracking Entries"
         }
       ]
     },
     "scripts": {
       "vscode:prepublish": "npm run compile",
       "compile": "tsc -p ./",
       "lint": "eslint src --ext ts",
       "watch": "tsc -watch -p ./"
     },
     "devDependencies": {
       "@types/vscode": "^1.60.0",
       "@types/node": "^14.14.37",
       "@typescript-eslint/eslint-plugin": "^4.14.1",
       "@typescript-eslint/parser": "^4.14.1",
       "eslint": "^7.19.0",
       "typescript": "^4.1.3"
     }
   }